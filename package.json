{
  "name": "test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:test": "npm run build:client && npm run build:server",
    "start": "vite-node server.ts",
    "build": "npm run lint:fix && tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "prettier": "npx prettier  src/**/*.{ts,tsx} --write",
    "lint:fix": "eslint src --fix",
    "test": "vitest --coverage",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "cy:run-e2e": "cypress run --e2e",
    "nyc": "npx nyc report --reporter=text-summary",
    "cy:e2e": "start-server-and-test start http-get://localhost:5173 cy:open-e2e",
    "cy:coverage": "start-server-and-test start http-get://localhost:5173 cy:run-e2e && nyc"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "compression": "^1.7.4",
    "cross-fetch": "^3.1.5",
    "express": "^4.18.2",
    "msw": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "sirv": "^2.0.2",
    "vite-plugin-eslint": "^1.8.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/react18": "^2.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "cross-env": "^7.0.3",
    "cypress": "^12.11.0",
    "cypress-vite": "^1.4.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.2"
  }
}
